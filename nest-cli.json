{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "apps/auction-app/src",
  "compilerOptions": {
    "deleteOutDir": true,
    "webpack": true,
    "tsConfigPath": "apps/auction-app/tsconfig.app.json"
  },
  "monorepo": true,
  "root": "apps/auction-app",
  "projects": {
    "auction-app": {
      "type": "application",
      "root": "apps/auction-app",
      "entryFile": "main",
      "sourceRoot": "apps/auction-app/src",
      "compilerOptions": {
        "tsConfigPath": "apps/auction-app/tsconfig.app.json"
      }
    },
    "gateway": {
      "type": "application",
      "root": "apps/gateway",
      "entryFile": "main",
      "sourceRoot": "apps/gateway/src",
      "compilerOptions": {
        "tsConfigPath": "apps/gateway/tsconfig.app.json"
      }
    },
    "auth": {
      "type": "application",
      "root": "apps/auth",
      "entryFile": "main",
      "sourceRoot": "apps/auth/src",
      "compilerOptions": {
        "tsConfigPath": "apps/auth/tsconfig.app.json"
      }
    },
    "nest": {
      "type": "application",
      "root": "apps/g/nest",
      "entryFile": "main",
      "sourceRoot": "apps/g/nest/src",
      "compilerOptions": {
        "tsConfigPath": "apps/g/nest/tsconfig.app.json"
      }
    },
    "bid": {
      "type": "application",
      "root": "apps/bid",
      "entryFile": "main",
      "sourceRoot": "apps/bid/src",
      "compilerOptions": {
        "tsConfigPath": "apps/bid/tsconfig.app.json"
      }
    },
    "common": {
      "type": "library",
      "root": "libs/common",
      "entryFile": "index",
      "sourceRoot": "libs/common/src",
      "compilerOptions": {
        "tsConfigPath": "libs/common/tsconfig.lib.json"
      }
    }
  }
}